{
  "name": "server",
  "version": "1.0.0",
  "description": "The proxy.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.17",
    "passport": "^0.4.0",
    "passport-auth0": "^1.1.0",
    "request": "^2.88.0",
    "rootpath": "^0.1.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.7"
  },
  "author": "meddlin",
  "license": "ISC"
}
